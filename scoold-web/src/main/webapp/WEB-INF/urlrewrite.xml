    </rule>
    <rule>
        <from>^/school/(\d+)/leave((/?)(.*?))$</from>
        <to>/school.htm?leave=true&amp;schoolid=$1</to>
    </rule>

	<!--SEO URLs-->
	<rule>
        <from>^/school/(\d+)/([^edit][^ask][^drawer][^photos][^classes][^people][^join][^leave])*.*(/?)$</from>
        <to>/school.htm?schoolid=$1</to>
