    </rule>
	<rule>
        <from>^/schools/votes(/?)$</from>
        <to>/schools.htm?sortby=votes</to>
    </rule>
	<rule>
        <from>^/schools/create(/?)$</from>
        <to>/schools.htm?create=true</to>
    </rule>

